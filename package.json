{
  "name": "brianzelip.github.io",
  "version": "1.0.0",
  "description": "Personal profile page of Brian Zelip",
  "main": "index.html",
  "scripts": {
    "build_css": "postcss -u postcss-import autoprefixer --autoprefixer.browsers '> 5%' -o css/zelip.css src/index.css",
    "minify_css": "postcss -u cssnano -o css/zelip.min.css css/zelip.css",
    "watch_css": "nodemon --watch src/ -e css -x \"npm run build_css && npm run minify_css\"",
    "serve": "http-server ./",
    "start": "npm run serve & npm run watch_css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-brianzelip:brianzelip/brianzelip.github.io"
  },
  "keywords": [
    "\"profile",
    "page\"",
    "\"Brian",
    "Zelip\""
  ],
  "author": "Brian Zelip",
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "basscss": "^7.1.1",
    "cssnano": "^3.7.7",
    "http-server": "^0.10.0",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.4",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.2"
  }
}
